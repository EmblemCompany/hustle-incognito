{
  "name": "hustle-incognito",
  "version": "0.3.0",
  "description": "SDK for interacting with the Emblem Vault Hustle Incognito Agent API",
  "author": "Emblem Vault",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "vitest run --exclude='**/integration.test.ts'",
    "test:all": "vitest run",
    "test:watch": "vitest --exclude='**/integration.test.ts'",
    "example:cli": "node examples/simple-cli.js",
    "example:cli:stream": "node examples/simple-cli.js --stream",
    "example:server": "node examples/simple-server.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmblemCompany/hustle-incognito.git"
  },
  "keywords": [
    "emblemvault",
    "hustle",
    "incognito",
    "ai",
    "sdk",
    "crypto",
    "solana"
  ],
  "bugs": {
    "url": "https://github.com/EmblemCompany/hustle-incognito/issues"
  },
  "homepage": "https://github.com/EmblemCompany/hustle-incognito#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/bun": "latest",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^16.4.5",
    "emblem-auth-sdk": "^1.0.0-alpha.6",
    "eslint": "^8.57.1",
    "ethers": "^5.8.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.4.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "file-type": "^21.1.0",
    "mcp-framework": "^0.2.11",
    "zod": "^3.24.3"
  }
}
